if contains $(cat /etc/environment) "HTTP_PROXY"
  export http_proxy=http://tyo4.sme.zscaler.net:80
  export https_proxy=${http_proxy}
  export ftp_proxy=${http_proxy}
  export HTTP_PROXY=${http_proxy}
  export HTTPS_PROXY=${http_proxy}
  export FTP_PROXY=${http_proxy}
end

if test -d ${HOME}/.cargo/bin && ! contains $PATH ${HOME}/.cargo/bin
  export PATH="$HOME/.cargo/bin:$PATH"
end

if ! contains $PATH ${HOME}/.config/ion/bin
  export PATH="$PATH:${HOME}/.config/ion/bin"
end